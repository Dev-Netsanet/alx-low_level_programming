#include <stdio.h>

/**
 * main - A program print size of various types
 *
 * Return: Always 0 (success)
 */
int main(void)
{
printf("size of a : %lu bytes(s)\n", sizeof(char));
printf("size of a : %lu bytes(s)\n", sizeof(int));
printf("size of a : %lu bytes(s)\n", sizeof(long int));
printf("size of a : %lu bytes(s)\n", sizeof(long long int));
printf("size of a : %lu bytes(s)\n", sizeof(float));
return (0);
}
